import java.io.*;
import java.util.*;
class Carrom {
	public static void main(String [] args) {
		int number;
		char c;
		Scanner sc = new Scanner(System.in);
		number = sc.nextInt();
		if(number < 1 || number>9) {
			System.out.println("Invalid Input");
		}
		else {
		c = sc.next().charAt(0);
		
			if(Character.compare(c, 'r') == 0) {
				System.out.println(number+5);
			}
			else {
				System.out.println(number);
			}
		}
	}
}
