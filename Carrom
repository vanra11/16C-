#include <iostream>

int main() {
    int number;
    char c;
    std::cin >> number;
    if (number < 1 || number > 9) {
        std::cout << "Invalid Input" << std::endl;
    } else {
        std::cin >> c;
        if (c == 'r') {
            std::cout << number + 5 << std::endl;
        } else {
            std::cout << number << std::endl;
        }
    }
    return 0;
}
